<div class="toast-container">
	@foreach (var toast in Toasts)
	{
		<Toast @key="@((toast.Id, toast.UniqueId))" 
			   Content="@toast.Content" 
			   Title="@toast.Title"
			   OnCloseCallback="() => RemoveToast(toast)" 
			   Duration="@toast.Duration" 
			   Type="@toast.ToastType"></Toast>
	}
</div>
